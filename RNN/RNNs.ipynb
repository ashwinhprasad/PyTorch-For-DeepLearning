   ],
   "source": [
    "#training loop\n",
    "for i in range(epochs):\n",
    "    for j,data in enumerate(train_loader):\n",
    "        y_pred = model(data[:][0].view(-1,10,1)).reshape(-1)\n",
    "        loss = criterion(y_pred,data[:][1])\n",
    "        loss.backward()\n",
    "        optimizer.step()\n",
    "    if i%50 == 0:\n",
    "        print(i,\"th iteration : \",loss)\n"
   ]
  },
  {
